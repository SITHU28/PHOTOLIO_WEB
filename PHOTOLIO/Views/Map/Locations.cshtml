@{
    ViewBag.Title = "Locations";
}
<script src="~/wwwroot/mine/js/map.js"></script>
<body>
    @*--------------------------------------------MAP---------------------------------------*@
    <section class="saction7">
        <div class="container">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="special">
                    <h2 class="os-animation" data-os-animation="bounceInDown" data-os-animation-delay="1s"><i class="fa fa-map-marker ST-text-blue "></i>  Locations where are nice palces to take photo. <i class="fa fa-camera ST-text-blue"></i></h2>
                </div>
                <br />
                <input type="text" id="txtDestination" value="" placeholder="Type a route name." style="width:200px;" class="os-animation" data-os-animation="fadeInDown" data-os-animation-delay="1s" />
                <input type="button" class="ST-button ST-round ST-blue os-animation ST-large" value="Search Route" onclick="SearchRoute()" style="width:200px;" data-os-animation="fadeInDown" data-os-animation-delay="1s" />
                <figure class="os-animation ST-card" data-os-animation="fadeInDown" data-os-animation-delay="0.50s">
                    <div id="googleMap" style="height:400px;width:100%;margin-top:30px;"></div>
                </figure>

            </div>
        </div>
        <br /><br />
        <div class="container">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="special">
                    <h2 class="os-animation" data-os-animation="bounceInDown" data-os-animation-delay="0.50s" style="margin-bottom:50px;"><i class="fa fa-map-signs ST-text-blue"></i> Guide To Use Our Map</h2>
                 </div>
            </div>
            <p class="food os-animation" data-os-animation="fadeInDown" data-os-animation-delay="1s">
               We have a location suggestion map for all our web user
            </p>
        </div>
    </section>
</body>

